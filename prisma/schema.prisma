generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model CATEMP {
  cteide Int @id @default(autoincrement())
  ctedes String
  ctesta String
  cademp CADEMP[]
}

model CADEND {
  cenide Int @id @default(autoincrement())
  cenrua String 
  cennum Int
  cenbai String 
  cencid String
  censta String
  cademp CADEMP[]
}

model CADEMP {
  cdeide Int @id @default(autoincrement())
  cdenom String
  cdecnp String
  categoria CATEMP @relation(fields: [cdecte], references: [cteide])
  cdecte Int 
  endereco CADEND @relation(fields:[cdecen], references: [cenide])
  cdecen Int 
  usuario CADUSU @relation(fields: [cdeusu], references: [cduide])
  cdeusu Int
  cdesta String
}

model CADPER {
  cdpide Int @id @default(autoincrement())
  cdpdes String
  cdpsta String
  usuario CADUSU[]
}

model CADUSU {
  cduide Int @id @default(autoincrement())
  cdunom String
  cducpf String
  cduema String @unique
  cdusen String
  perfil CADPER @relation(fields: [cduper], references: [cdpide])
  cduper Int
  cdufac String
  cduins String
  cdutel String
  cdusta String
  empresa CADEMP[]
}